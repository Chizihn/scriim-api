{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "outputDirectory": "dist",
  "env": {
    "MONGODB_URI": "@mongodb-uri",
    "TERMII_API_KEY": "@termii-api-key",
    "TERMII_API_URL": "@termii-api-url",
    "EMAIL_USER": "@email-user",
    "EMAIL_PASS": "@email-pass"
  }
}
